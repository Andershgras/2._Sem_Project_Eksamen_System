@page
@model _2._Sem_Project_Eksamen_System.Pages.Eksamner.GetEksamnerModel
@{
    ViewData["Title"] = "GetEksamner";
}

<h1>GetEksamner</h1>
<div>
    @foreach (var e in Model.Eksamner)
    {
        <div class="card">
            <h5 class="card-header">Featured</h5>
            <div class="card-body">
                <h5 class="card-title">Special title treatment</h5>
                <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
                <a href="#" class="btn btn-primary">Go somewhere</a>
            </div>
        </div>
    }
</div>


<table class="table">
    <thead>
        <tr>
            <th> Eksamens ID</th>
            <th> Title</th>
            <th> Hold ID</th>
            <th> Dato</th>
        </tr>
    </thead>
    <tbody>
       @false
        @foreach (var eksamen in Model.Eksamner)
        {
            <tr>
                <td>@eksamen.ExamId</td>
                <td>@eksamen.ExamName</td>
                <td>@eksamen.ClassId</td>
                <td>@eksamen.ExamStartDate</td>
            </tr>
        }
    </tbody>
</table>
